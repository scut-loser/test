{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MarketTick",
  "type": "object",
  "required": ["ts", "symbol", "price", "volume", "open_interest"],
  "properties": {
    "ts": { "type": "string", "description": "ISO8601 timestamp" },
    "symbol": { "type": "string" },
    "price": { "type": "number" },
    "volume": { "type": "number" },
    "open_interest": { "type": "number" }
  }
}
